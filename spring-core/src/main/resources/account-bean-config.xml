<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="
    http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="accountService" class="com.example.springcoredojo.examplexmlpath.Youtuber">
        <constructor-arg name="accountRepository" ref="accountRepository" />
    </bean>

    <bean id="accountRepository" class="com.example.springcoredojo.examplexmlpath.AccountRepository" >
        <property name="name" value="nombre"/>
    </bean>


</beans>